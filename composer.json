{
    "name": "fuliang/php-tools",
    "description": "php开发辅助类以及函数",
    "type": "composer-plugin",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fuliang\\PhpTools\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "authors": [
        {
            "name": "LingZi",
            "email": "idevjdgittyucidvdinoper007@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-gd": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {},
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false
        },
        "tools": {
            "helper": [
                "Fuliang\\PhpTools\\Helper\\ArrayHelper"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
